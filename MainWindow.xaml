                <Button Content="*" HorizontalAlignment="Left" Margin="153,44,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="star"/>
                <Button Content="7" HorizontalAlignment="Left" Margin="0,91,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="seven"/>
                <Button Content="8" HorizontalAlignment="Left" Margin="51,91,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="eight"/>
                <Button Content="9" HorizontalAlignment="Left" Margin="102,91,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="nine"/>
                <Button Content="-" HorizontalAlignment="Left" Margin="153,91,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="subtract"/>
                <Button Content="4" HorizontalAlignment="Left" Margin="0,128,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="four"/>
                <Button Content="5" HorizontalAlignment="Left" Margin="51,128,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="five"/>
                <Button Content="6" HorizontalAlignment="Left" Margin="102,128,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="six"/>
