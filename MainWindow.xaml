                <Button Content="3" HorizontalAlignment="Left" Margin="102,165,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="three"/>
                <Button Content="=" HorizontalAlignment="Left" Margin="153,165,0,0" VerticalAlignment="Top" Width="46" Height="69" Click="equals"/>
                <Button Content="+" HorizontalAlignment="Left" Margin="152,128,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="add"/>
                <Button Content="." HorizontalAlignment="Left" Margin="102,202,0,0" VerticalAlignment="Top" Width="46" Height="32" Click="dec"/>
                <Button Content="0" HorizontalAlignment="Left" Margin="0,202,0,0" VerticalAlignment="Top" Width="97" Height="32" Click="zero"/>

            </Grid>

        </Grid>
    </Grid>
